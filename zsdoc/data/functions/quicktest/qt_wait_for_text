local test_name="$1"
local text="$2"  
local retries=$3
local interval=$4
for c in $( seq 1 $retries ); do
    qt_echo "ğŸ” $c / $retries Â«$textÂ» "
    qt_screenshot_ppm "$test_name"
    if qt_search_screenshot "$test_name" "$text"; then
        return 0
    fi
    qt_wait_for_seconds "$interval"
done
qt_test_fail "Coulnd't find text Â«$textÂ» in screenshot Â«$QT_SCREENSHOT_COUNTÂ» after $retries retries"
