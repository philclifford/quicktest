if [ ! -f "${QT_TESTCASES_DIR}"/"${OS}"/"${RELEASE}${EDITIONPATH}"/"${TESTCASE}" ]; then
    qt_echo "ðŸš¨ Test case ${TESTCASE} does not exist for ${OS}-${RELEASE}${EDITIONSUFFIX}."
    exit 1
fi
if source "${QT_TESTCASES_DIR}"/"${OS}"/"${RELEASE}${EDITIONPATH}"/"${TESTCASE}"; then
    qt_echo "âœ… Source quicktest scripts succeeded."
    return 0
else
    qt_echo "ðŸš¨ Source quicktest scripts failed."
    return 1
fi
